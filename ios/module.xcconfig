MYMODVERSION=1.0.0

FRAMEWORK_SEARCH_PATHS=$(inherited) ~/Documents/FacebookSDK $(SRCROOT)/../../modules/iphone/$(MYMODID)/$(MYMODVERSION)/platform /Library/Application\ Support/Titanium/modules/iphone/$(MYMODID)/$(MYMODVERSION)/platform ~/Library/Application\ Support/Titanium/modules/iphone/$(MYMODID)/$(MYMODVERSION)/platform
OTHER_LDFLAGS=$(inherited) -framework FBSDKLoginKit -framework FBSDKCoreKit -framework GigyaSDK  -framework CoreData -framework Social
